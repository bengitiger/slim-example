{
    "name":         "fr05t1k/slim-example",
    "description":  "A Slim Framework skeleton application for rapid development",
    "keywords":     [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage":     "http://github.com/slimphp/Slim-Skeleton",
    "license":      "MIT",
    "authors":      [
        {
            "name":  "Stanislav Pavlovichev",
            "email": "spavlovichev@gmail.com"
        }
    ],
    "autoload":     {
        "psr-4": {
            "Fr05t1k\\SlimExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fr05t1k\\SlimExampleTests\\": "tests/"
        }
    },
    "require":      {
        "php":                         ">=5.5.0",
        "roave/security-advisories":   "dev-master",
        "slim/slim":                   "^3.1",
        "monolog/monolog":             "^1.17",
        "php-di/slim-bridge":          "^1.0",
        "doctrine/cache":              "^1.4",
        "vlucas/phpdotenv":            "^2.4.0",
        "mongodb/mongodb":             "^1.1.0",
        "facebook/graph-sdk":          "^5.2",
        "zendframework/zend-hydrator": "^2.2.0",
        "symfony/console":             "^3.2.0"
    },
    "require-dev":  {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "mockery/mockery": "^0.9.7"
    },
    "config":       {
        "bin-dir": "bin"
    },
    "scripts":      {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test":  "phpunit ${*:2}"
    }
}
